{
  "jobs": [
    {
      "command": "0 3 * * 1-5 bundle exec ruby main.rb --app rdv_solidarites"
    },
    {
      "command": "0 4 * * 1-5 [[ -z \"$SKIP_RDV_INSERTION\" ]] && bundle exec ruby main.rb --app rdv_insertion",
      "_comment": "// we want to disable rdv_insertion ETL on demo because it is hosted on a different region"
    },
    {
      "command": "30 4 * * 1-5 bundle exec ruby main.rb --app rdv_service_public"
    },
  ]
}
